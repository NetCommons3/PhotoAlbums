{
    "name": "netcommons/photo-albums",
    "description": "PhotoAlbums for NetCommons Plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "photoAlbums"],
    "homepage": "http://www.netcommons.org/",
    "license": "NetCommons License",
    "authors": [
        {
            "name": "Kohei Teraguchi",
            "email": "kteraguchi@commonsnet.org",
            "role": "Developer"
        },
        {
            "name": "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/PhotoAlbums/graphs/contributors"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakephp/cakephp": "~2.6",
        "cakedc/migrations": "~2.2",
        "netcommons/net-commons": "dev-master",
        "netcommons/plugin-manager": "dev-master",
        "netcommons/users": "dev-master",
        "netcommons/blocks": "dev-master",
        "netcommons/frames": "dev-master",
        "netcommons/workflow": "dev-master",
        "netcommons/files": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7.38",
        "cakephp/debug_kit": "~2.2",
        "satooshi/php-coveralls": "dev-master"
    },
    "config": {
        "vendor-dir": "vendors"
    }
}
